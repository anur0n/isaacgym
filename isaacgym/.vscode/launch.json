            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Docker: Python - isaacgym",
            "type": "docker",
            "request": "launch",
            "preLaunchTask": "docker-run: debug",
            "python": {
              "pathMappings": [
                {
                  "localRoot": "${workspaceFolder}",
                  "remoteRoot": "/app"
